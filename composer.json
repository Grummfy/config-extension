    "keywords": ["TDD", "atoum", "test", "unit testing", "config", "atoum-extension"],
    "homepage": "http://www.atoum.org",
    "license": "BSD",
    "authors": [
        {
            "name": "jubianchi",
            "email": "contact@jubianchi.fr"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "atoum/atoum": "^2.9|^3.0",
